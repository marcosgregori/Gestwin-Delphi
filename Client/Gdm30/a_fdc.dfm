  object LineasAlbaranQuery: TnxeQuery
    Database = EnterpriseDataModule.UserDatabase
    Timeout = 180000
    AfterOpen = LineasAlbaranQueryAfterOpen
    AfterClose = LineasAlbaranQueryAfterClose
    DataSource = FacturaComprasDataSource
    SQL.Strings = (
      'SELECT Movimiento.Serie,'
      '       Movimiento.NroDocumento,'
      '       Movimiento.Fecha,'
      ''
      '       LineaMovimiento.*'
      ''
      'FROM  Movimiento'
      
        '      LEFT JOIN LineaMovimiento ON ( Movimiento.Ejercicio=LineaM' +
        'ovimiento.Ejercicio and Movimiento.NroOperacion=LineaMovimiento.' +
        'NroOperacion )'
      ''
      'WHERE Movimiento.TipoMovimiento=12 AND'
      '      Movimiento.EjercicioFactura=:Ejercicio AND'
      '      Movimiento.PropietarioFactura=:Proveedor AND'
      '      Movimiento.SerieFactura=:SerieNroFactura'
      ''
      'ORDER BY'
      '      Ejercicio, NroDocumento, NroLinea')
