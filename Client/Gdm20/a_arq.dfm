      'SELECT LineaArqueo.*, FormaCobro.Descripcion'
      
        'FROM LineaArqueo LEFT JOIN FormaCobro ON ( LineaArqueo.FormaCobr' +
        'o=FormaCobro.Codigo )'
      
        'WHERE Serie=:Serie AND Fecha=:Fecha AND TipoOperacion=10 // oaVe' +
        'nta'
